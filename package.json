{
  "name":        "@mnasyrov/emoji-data",
  "version":     "0.2.0",
  "description": "Emoji encoding swiss army knife for NodeJS (forked from mroth/emoji-data-js)",
  "keywords": [
    "emoji",
    "unicode"
  ],
  "homepage": "https://github.com/mnasyrov/emoji-data-js",
